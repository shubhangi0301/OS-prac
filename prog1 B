#include <sys/types.h>
#include <stdio.h>
#include <unistd.h>
int main()
{
    pid_t pid;
    if (pid < 0)
    {
		fprintf(stderr, "Fork Failed");
        return 1;
    }
    else if (pid == 0)
    {
		execlp("/bin/ls","ls",NULL);
    }
    else
    {
        printf("Parent Process.");
    }
    return 0;
}
